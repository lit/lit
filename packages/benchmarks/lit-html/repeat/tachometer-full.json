{
  "$schema": "https://raw.githubusercontent.com/Polymer/tachometer/master/config.schema.json",
  "sampleSize": 30,
  "timeout": 0,
  "benchmarks": [
    {
      "url": "lit-html/repeat/index.html",
      "browser": {
        "name": "chrome",
        "headless": true
      },
      "measurement": [
        {
          "mode": "performance",
          "entryName": "render"
        },
        {
          "mode": "performance",
          "entryName": "nop"
        },
        {
          "mode": "performance",
          "entryName": "add"
        },
        {
          "mode": "performance",
          "entryName": "remove"
        },
        {
          "mode": "performance",
          "entryName": "addEdges"
        },
        {
          "mode": "performance",
          "entryName": "removeEdges"
        },
        {
          "mode": "performance",
          "entryName": "addMirror"
        },
        {
          "mode": "performance",
          "entryName": "removeMirror"
        },
        {
          "mode": "performance",
          "entryName": "addStripe"
        },
        {
          "mode": "performance",
          "entryName": "removeStripe"
        },
        {
          "mode": "performance",
          "entryName": "swapOne"
        },
        {
          "mode": "performance",
          "entryName": "swapMany"
        },
        {
          "mode": "performance",
          "entryName": "swapEdges"
        },
        {
          "mode": "performance",
          "entryName": "moveForwardOne"
        },
        {
          "mode": "performance",
          "entryName": "moveForwardMany"
        },
        {
          "mode": "performance",
          "entryName": "moveBackwardOne"
        },
        {
          "mode": "performance",
          "entryName": "moveBackwardMany"
        },
        {
          "mode": "performance",
          "entryName": "reverse"
        },
        {
          "mode": "performance",
          "entryName": "shuffle"
        },
        {
          "mode": "performance",
          "entryName": "replace"
        },
        {
          "mode": "performance",
          "entryName": "replaceStripe"
        },
        {
          "mode": "performance",
          "entryName": "removeAll"
        }
      ],
      "expand": [
        {
          "name": "this-change"
        },
        {
          "name": "tip-of-tree",
          "packageVersions": {
            "label": "tip-of-tree",
            "dependencies": {
              "lit-html": {
                "kind": "git",
                "repo": "https://github.com/Polymer/lit-html.git",
                "ref": "lit-next",
                "subdir": "packages/lit-html",
                "setupCommands": [
                  "npm ci",
                  "npm run bootstrap -- --concurrency=1",
                  "npm run build"
                ]
              },
              "lit-element": {
                "kind": "git",
                "repo": "https://github.com/Polymer/lit-html.git",
                "ref": "lit-next",
                "subdir": "packages/lit-element",
                "setupCommands": [
                  "npm ci",
                  "npm run bootstrap -- --concurrency=1",
                  "npm run build"
                ]
              },
              "@lit/reactive-element": {
                "kind": "git",
                "repo": "https://github.com/Polymer/lit-html.git",
                "ref": "lit-next",
                "subdir": "packages/reactive-element",
                "setupCommands": [
                  "npm ci",
                  "npm run bootstrap -- --concurrency=1",
                  "npm run build"
                ]
              }
            }
          }
        },
        {
          "name": "previous-release",
          "packageVersions": {
            "label": "previous-release",
            "dependencies": {
              "lit-html": "^1.0.0",
              "lit-element": "^2.0.0",
              "@lit/reactive-element": {
                "kind": "git",
                "repo": "https://github.com/Polymer/lit-html.git",
                "ref": "lit-next",
                "subdir": "packages/reactive-element",
                "setupCommands": [
                  "npm ci",
                  "npm run bootstrap -- --concurrency=1",
                  "npm run build"
                ]
              }
            }
          }
        }
      ]
    }
  ]
}
