{
  "name": "@lit-labs/ssr-koa-middleware",
  "version": "1.0.0-pre.1",
  "description": "Koa middleware for rendering HTML files using @lit-labs/ssr.",
  "license": "BSD-3-Clause",
  "homepage": "https://lit-element.polymer-project.org/",
  "repository": "lit-html",
  "type": "module",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "directories": {
    "test": "test"
  },
  "exports": {
    ".": {
      "development": "./development/index.js",
      "default": "./index.js"
    }
  },
  "files": [
    "/src/",
    "!/src/test/",
    "/index.{d.ts,d.ts.map,js,js.map}",
    "/render-html-file.{d.ts,d.ts.map,js,js.map}"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:ts",
    "build:ts": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf {index,render-html-file}.{js,js.map,d.ts} test/ *.tsbuildinfo",
    "regen-package-lock": "rm -rf node_modules package-lock.json; npm install"
  },
  "author": "Google LLC",
  "devDependencies": {
    "@types/koa": "^2.13.1",
    "koa": "^2.13.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@lit-labs/ssr": "^1.0.0-pre.1",
    "parse5": "^5.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
