  ],
  "dependencies": {
    "lit": "^3.0.0"
  },
  "devDependencies": {
    "typescript": "~5.2.0"
  },
  "wireit": {
    "build": {
      "dependencies": [
        "../../../lit:build"
      ],
      "files": [
        "src/**/*.ts",
